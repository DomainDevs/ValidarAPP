    <collection name="CoRequest" type="Sistran.Company.Application.Request.Entities.CoRequest,
                Company.Request.Entities"></collection>
    <collection name="CoRequestEndorsement" type="Sistran.Company.Application.Request.Entities.CoRequestEndorsement,
                Company.Request.Entities"></collection>
    <collection name="CoRequestEndorsementAgent" type="Sistran.Company.Application.Request.Entities.CoRequestEndorsementAgent,
                Company.Request.Entities"></collection>
    <collection name="CoRequestAgent" type="Sistran.Company.Application.Request.Entities.CoRequestAgent,
                Company.Request.Entities"></collection>
    <collection name="Agent" type="Sistran.Core.Application.UniquePerson.Entities.Agent,
                Core.UniquePerson.Entities"></collection>
    <collection name="BillingGroup" type="Sistran.Core.Application.Issuance.Entities.BillingGroup, 
                Core.Issuance.Entities"></collection>
