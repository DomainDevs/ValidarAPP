    <relation source="CoRequestEndorsement" destination="CoRequestEndorsementAgent" name="CoRequestEndorsementCoRequestEndorsementAgent" type="Inner">
      <property left="RequestEndorsementId" type="Equal" right="RequestEndorsementId"></property>
    </relation>
    <relation source="CoRequestEndorsementAgent" destination="CoRequestAgent" name="CoRequestEndorsementAgentCoRequestAgent" type="Inner">
      <property left="RequestAgentId" type="Equal" right="RequestAgentId"></property>
    </relation>    
    <relation source="CoRequestAgent" destination="Agent" name="CoRequestAgentAgent" type="Inner">
      <property left="IndividualId" type="Equal" right="IndividualId"></property>
    </relation>
    <relation source="CoRequest" destination="BillingGroup" name="CoRequestBillingGroup" type="Inner">
      <property left="BillingGroupCode" type="Equal" right="BillingGroupCode"></property>
    </relation>
  </relations>
</view>