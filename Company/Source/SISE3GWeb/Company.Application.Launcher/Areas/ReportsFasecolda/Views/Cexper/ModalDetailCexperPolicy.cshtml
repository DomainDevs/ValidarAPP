            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelNroPolicy</label>
                <input type="text" disabled name="NumberPolice" id="inputModalNumberPolice" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.Order</label>
                <input type="text" disabled name="Order" id="inputModalOrder" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelStatus</label>
                <input type="text" disabled name="company" id="inputModalCompany" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.StartValidateDate</label>
