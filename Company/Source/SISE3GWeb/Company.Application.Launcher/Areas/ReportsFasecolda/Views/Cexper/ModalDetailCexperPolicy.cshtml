                <input type="text" disabled name="PolicyClass" id="inputModalLabelPolicyClass" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="col-md-12 text-center">
                <label>
                    @Resources.Language.LabelDataVehicle
                </label>
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelPlate</label>
                <input type="text" disabled name="Plate" id="inputModalPlate" value="" />
            </div>
            <div class="uif-col-4">
