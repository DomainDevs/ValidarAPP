        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelTypeDocument</label>
                <input type="text" disabled name="DocumentTypeBeneficiary" id="inputModalDocumentTypeBeneficiary" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelDocument</label>
                <input type="text" disabled name="DocumentBeneficiary" id="inputModalDocumentBeneficiary" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelTableName</label>
                <input type="text" disabled name="NameBeneficiary" id="inputModalNameBeneficiary" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-md-12 text-center">
                <label>
                    @Resources.Language.LabelInformationAmparos
                </label>
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-12">
                <table class="uif-table" id="TableInfoProtection">
                    <thead>
                        <tr>
                            <th data-property="Amparo">@Resources.Language.LabelProtection</th>
                            <th data-property="Notice">@Resources.Language.LabelAdvice</th>
                            <th data-property="status">@Resources.Language.LabelStatus</th>
                            <th data-property="Value">@Resources.Language.Value</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
       
        <br />
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelInsuredValue</label>
                <input type="text" disabled name="InsuredValue" id="inputModalInsuredValue" value="" />
            </div>
            <div class="uif-col-3">
                <label>@Resources.Language.LabelTotalClaim</label>
                <input type="text" disabled name="TotalClaim" id="inputModalTotalClaim" value="" />
            </div>
        </div>
        <div class="uif-row">
        </div>
    </div>

    <div class="uif-modalbuttons width-save">
        <button type="button" class="btn btn-default" data-dismiss="modal">@Resources.Language.Close</button>
        <button type="button" class="btn btn-primary" id="btnExportModalPolicyInformation">@Resources.Language.ButtonExport</button>
    </div>
</div>