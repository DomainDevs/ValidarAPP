@{
    Layout = null;
}

<div class="uif-modal" id="ModalSinisterInformation">
    <div class="uif-modalbody">
        <div class="uif-row">
            <div class="col-md-12 text-center">
                <label>
                    @Resources.Language.LabelPolicyInformation
                </label>
            </div>
        </div>
        <div class="uif-row">

            <div class="uif-col-4">
                <label> @Resources.Language.LabelCompany</label>
                <input type="text" disabled name="company" id="inputModalCompany" value="" />
            </div>
            <div class="uif-col-4">
                <label> @Resources.Language.LabelNroPolicy</label>
                <input type="text" disabled name="NumberPolice" id="inputModalNumberPolice" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.Order</label>
                <input type="text" disabled name="Order" id="inputModalOrder" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.Status</label>
                <input type="text" disabled name="company" id="inputModalCompany" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.BeginDate</label>
                <input type="text" disabled name="SValidity" id="inputModalSValidity" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.EndValidateDate</label>
                <input type="text" disabled name="EValidity" id="inputModalEValidity" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="col-md-12 text-center">
                <label>
                    @Resources.Language.LabelDataVehicle
                </label>
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelLicencesePlate</label>
                <input type="text" disabled name="Plate" id="inputModalPlate" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelMake</label>
                <input type="text" disabled name="Brand" id="inputModalBrand" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelClass</label>
                <input type="text" disabled name="Clase" id="inputModalClase" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelModel</label>
                <input type="text" disabled name="Model" id="inputModalModel" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelEngine</label>
                <input type="text" disabled name="Motor" id="inputModalMotor" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelChassis</label>
                <input type="text" disabled name="Chassis" id="inputModalChassis" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelType</label>
                <input type="text" disabled name="Type" id="inputModalType" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="col-md-12 text-center">
                <label>
                    @Resources.Language.LabelInsuredDetail
                </label>
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelTypeDocument</label>
                <input type="text" disabled name="DocumentTypeInsured" id="inputModalDocumentTypeInsured" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelDocument</label>
                <input type="text" disabled name="DocumentInsured" id="inputModalDocumentInsured" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelTableName</label>
                <input type="text" disabled name="NameInsured" id="inputModalNameInsured" value="" />
            </div>
        </div>
        <div class="uif-row">
            <div class="col-md-12 text-center">
                <label>
                    @Resources.Language.LabelHolderDetail
                </label>
            </div>
        </div>
        <div class="uif-row">
            <div class="uif-col-4">
                <label>@Resources.Language.LabelTypeDocument</label>
                <input type="text" disabled name="DocumentTypeTaker" id="inputModalDocumentTypeTaker" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelDocument</label>
                <input type="text" disabled name="DocumentTaker" id="inputModalDocumentTaker" value="" />
            </div>
            <div class="uif-col-4">
                <label>@Resources.Language.LabelTableName</label>
                <input type="text" disabled name="NameTaker" id="inputModalNameTaker" value="" />
