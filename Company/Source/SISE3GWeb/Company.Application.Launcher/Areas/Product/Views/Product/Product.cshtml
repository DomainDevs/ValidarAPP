@model Sistran.Core.Framework.UIF.Web.Areas.Product.Models.ProductModelsView
@section Title{
    <div id="mainProduct" class="row main-title">
        <div class="column-40">
            <div id="Title">@Resources.Language.TitleProducts</div>
        </div>
        <div class="column-30 no-padding">
            <select class="uif-select important" id="selectSearchProduct" data-val="true" data-source='@Url.Content("~/Product/Product/GetPrefixes")' data-val-required="@Resources.Language.ErrorDocumentPrefix"
                    data-id="Id" data-name="Description" name="PrefixId" data-emptyText="@Resources.Language.SelectABranch"></select>
        </div>
        <div class="column-30 even">
            @Html.TextBox("Description", null, new { @class = "uif-input-search", id = "inputProduct", @placeholder = @Resources.Language.ProductName, maxlength = "50" })
            <button class="btn btn-default" id="btnSearchAdvProduct"><i class="fa fa-angle-down"></i></button>
        </div>
    </div>
    <div id="mainProductNew" class="row main-title " style="display: none">
        <div class="column-70">
            <div id="globalTitle">@Resources.Language.TitleProducts</div>
        </div>
        <div class="column-30 even" style="justify-content:space-between">
            <div id="globalText">@Resources.Language.TitleProducts</div>
        </div>

    </div>
}
@section Toolbar
{
    <footer>
        <div class="app-header">
            <div class="row">
                <div class="uif-col-12">
                    <div id="buttonsProduct" class="pull-right">
                        <button class="btn btn-default" id="btnReturnProduct">
                            <span class="fa fa-times-circle" aria-hidden="true"></span>
                            @Resources.Language.ButtonExit
                        </button>
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0612"))
                        {
                            <button class="btn btn-default" id="btnProductNew">
                                <span class="fa fa-file-o" aria-hidden="true"></span>
                                @Resources.Language.LabelNewProduct
                            </button>
                        }

                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0611"))
                        {
                            <button class="btn btn-default" id="btnReportProduct">
                                <i class="fa fa-cloud-download" aria-hidden="true"></i>
                                <span class="" aria-hidden="true"></span>
                                @Resources.Language.ButtonReport
                            </button>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0610"))
                        {
                            <button class="btn btn-default" id="btnListProduct">
                                <i class="fa fa-cloud-download" aria-hidden="true"></i>
                                <span class="" aria-hidden="true"></span>
                                @Resources.Language.ButtonListProduct
                            </button>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0609"))
                        {
                            <button class="btn btn-default" id="btnProductEnableDisableBtn">
                                <span class="fa fa-ban" aria-hidden="true"></span>
                                <span id="btnProductEnableDisable">@Resources.Language.LabelDisable</span>
                            </button>
                        }
                        <button type="button" class="btn btn-default" id="btnCancel" style="display:none">
                            <span class="" aria-hidden="true"></span>
                            @Resources.Language.ButtonWipeScreen
                        </button>
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0608"))
                        {
                            <button class="btn btn-primary" id="btnRecordProduct">
                                <span class="fa fa-floppy-o" aria-hidden="true"></span>
                                @Resources.Language.LabelSavedProduct
                            </button>
                        }
                    </div>
                    <div id="buttonsInsuredObject" class="pull-right">
                        <button type="button" class="btn btn-default" id="btnModalInsuredObjectClose">@Resources.Language.ButtonClose</button>
                        <button type="button" class="btn btn-primary" id="btnModalInsuredObjectSave">@Resources.Language.ButtonSave</button>
                    </div>
                    <div id="buttonsCoverage" class="pull-right">
                        <button type="button" class="btn btn-default" id="btnModalCoverageClose">@Resources.Language.ButtonClose</button>
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0594"))
