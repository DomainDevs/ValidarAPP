                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0593"))
                        {
                            <button type="button" class="btn btn-primary" id="btnModalCoverageSave">@Resources.Language.ButtonSave</button>
                        }
                    </div>
                    <div id="buttonsRulesAndscripts" class="pull-right">
                        <button type="button" class="btn btn-default" id="btnModalRulesAndscriptsClose">@Resources.Language.ButtonClose</button>
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0597"))
                        {
                            <button type="button" class="btn btn-primary" id="btnModalRulesAndscriptsSave">@Resources.Language.ButtonSave</button>
                        }
                    </div>
                    <div id="buttonsAdditionalData" class="pull-right">
                        <button type="button" class="btn btn-default" id="btnModalAdditionalDataClose">@Resources.Language.ButtonClose</button>
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0598"))
                        {
                            <button type="button" class="btn btn-primary" id="btnModalAdditionalDataSave">@Resources.Language.ButtonSave</button>
                        }
                    </div>
                    <div id="buttonsAgents" class="pull-right">
                        <button type="button" class="btn btn-default" id="btnModalAgentsClose">@Resources.Language.ButtonClose</button>

                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0606"))
                        {
                            <button type="button" class="btn btn-default" id="btnModalAgentsDeallocateAll">@Resources.Language.ButtonDeallocateAll</button>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0605"))
                        {
                            <button type="button" class="btn btn-default" id="btnModalAgentsAssignAll">@Resources.Language.ButtonAssignAll</button>
                        }

                        @*<button type="button" class="btn btn-primary" id="btnModalAgentsSave">@Resources.Language.ButtonSave</button>*@
                    </div>
                </div>
            </div>
        </div>
    </footer>
}

