<form id="formProduct" class="form form-space-adjusted" autocomplete="off">
    <div id="modalProduct">
        @Html.HiddenFor(model => model.Title, new { id = "hiddenTitle" })
        <div class="row">
            <div class="uif-col-9">
                <div class="row row-space-adjusted">
                    <div class="uif-col-6">
                        <label class="field-required">@Resources.Language.LabelPrefix</label>

                        <select class="uif-select form-control" id="selectPrefixCommercial" data-val="true"
                                data-source='@Url.Content("~/Product/Product/GetPrefixes")'
                                data-val-required="@Resources.Language.ErrorDocumentControlPrefixCommercial"
                                data-id="Id" data-name="Description" name="PrefixId" data-filter="true" data-native="false"></select>
                        @Html.ValidationMessageFor(model => model.PrefixId)
                    </div>
                    <div class="uif-col-6">
                        <label class="field-required">@Resources.Language.LabelDescriptionProduct</label>
                        @Html.TextBoxFor(model => model.Description, new { id = "inputDescription", placeholder = @Resources.Language.LabelDescriptionProduct, maxlength = "50" })
                        @Html.ValidationMessageFor(model => model.Description)
                    </div>

                </div>
                <div class="row">
                    <div class="uif-col-3">
                        <label class="field-required">@Resources.Language.LabelDescriptionProductReduced</label>
                        @Html.TextBoxFor(model => model.DescriptionReduced, new { id = "inputDescriptionReduced", placeholder = @Resources.Language.LabelDescriptionProductReduced, maxlength = "15" })
                        @Html.ValidationMessageFor(model => model.DescriptionReduced)
                    </div>

                    <div class="uif-col-3">
                        <label class="field-required">@Resources.Language.LabelMoney</label>
                        <div class="input-with-buttons">
                            <select class="uif-select form-control" id="selectCurrency" data-val="true" data-val-required="@Resources.Language.ErrorDocumentControlMoney"
                                    data-id="Id" data-name="Description" name="Currency" data-filter="true" data-native="false"></select>
                            @Html.ValidationMessageFor(model => model.Currency)
                            <button type="button" class="btn btn-default" id="btnCurrency">
                                <i class="fa fa-plus" aria-hidden="true"></i>
                            </button>
                        </div>
                    </div>








                    <div class="uif-col-3">
                        <label class="field-required">@Resources.Language.LabelCurrentDate</label>
                        @Html.TextBoxFor(model => model.CurrentDate, new { id = "inputCurrentDate", @class = "uif-datepicker", @readonly = "readonly" })
                        @Html.ValidationMessageFor(model => model.CurrentDate)
                    </div>
                    <div class="uif-col-3">
                        <label>@Resources.Language.LabelDisabledDate</label>
                        @Html.TextBoxFor(model => model.DisabledDate, new { id = "inputDisabledDate", @class = "uif-datepicker" })
                    </div>
                </div>
                <div class="row">
                    <div class="uif-col-6">
                        <label class="field-required">@Resources.Language.LabelPolicyType</label>
                        <div class="input-with-buttons">
                            <select class="uif-select form-control" id="selectPolicyType" data-val="true" data-val-required="@Resources.Language.ErrorDocumentControlPolicyType"
                                    data-id="Id" data-name="Description" name="PolicyType" data-filter="true" data-native="false"></select>
                            @Html.ValidationMessageFor(model => model.PolicyType)
                            <button type="button" class="btn btn-default" id="btnPolicyType">
                                <i class="fa fa-plus" aria-hidden="true"></i>
                            </button>
                        </div>
                    </div>
                    <div class="uif-col-4">
                        <label class="field-required">@Resources.Language.LabelRiskType</label>
                        <div class="input-with-buttons">
                            <select class="uif-select form-control" id="selectRiskType" data-val="true" data-val-required="@Resources.Language.ErrorDocumentControlRiskType"
                                    data-id="Id" data-name="Description" name="RiskType" data-filter="true" data-native="false"></select>
                            @Html.ValidationMessageFor(model => model.RiskType)
                            <button type="button" class="btn btn-default" id="btnRiskType">
                                <i class="fa fa-plus" aria-hidden="true"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="uif-col-3">
                        <label class="field-required">@Resources.Language.LabelMaximumNumberRisk</label>
                        @Html.TextBoxFor(model => model.MaximumNumberRisk, new { id = "inputMaximumNumberRisk", @class = "rightAlign", onKeyPress = "return checkOnlyNumbersLength(event,value)" })
                        @Html.ValidationMessageFor(model => model.MaximumNumberRisk)
                    </div>

                    <div class="uif-col-2">
                        <label>@Resources.Language.LabelVersion</label>
                        @Html.TextBoxFor(model => model.VersionId, new { id = "inputVersionId", placeholder = @Resources.Language.LabelVersion, @readonly = "readonly", disabled = "disabled", Value = "1" })
                    </div>

                    <div class="uif-col-3">
                        <label>@Resources.Language.LabelProduct2G</label>
                        <select class="uif-select form-control" id="selectProduct2G"
                                data-id="Id" data-name="Description" name="Product2G" data-filter="true" data-native="false"></select>

                    </div>

                    <div class="uif-col-3">
                        <label class="field-required">@Resources.Language.LabelPercentageCommissionNormal</label>
                        @Html.TextBoxFor(model => model.Percentage, new { id = "inputProductAgentsPercentage", @class = "rightAlign" })
                        @Html.ValidationMessageFor(model => model.Percentage)
                    </div>
                    <div class="uif-col-3">
                        <label>@Resources.Language.LabelPercentageCommissionAditional</label>
                        @Html.TextBoxFor(model => model.PercentageAdditional, new { id = "inputProductAgentsPercentageAdditional", @class = "rightAlign" })
                        @Html.ValidationMessageFor(model => model.PercentageAdditional)
                    </div>


                </div>


                <div class="row">
                    <div class="uif-col-6" style="padding-top:2%">
                    </div>

                </div>



                <div class="row">
                    <div class="uif-col-12">
                        <label>@Resources.Language.ApplyTo :</label>
                    </div>
                </div>
                <div class="row">
                    <div class="uif-col-3">
                        <input type="checkbox" id="chkLightCotizador" name="LightCotizador" disabled="disabled" />
                        <label for="chkLightCotizador">@Resources.Language.LabelApplyLightCotizador</label>
                    </div>
                    <div class="uif-col-3">
                        <input type="checkbox" id="chkFlatRate" name="FlatRate" disabled="disabled" />
                        <label for="chkFlatRate">@Resources.Language.LabelFlatRateProduct</label>
                    </div>

                </div>
                <div class="row">
                    <div class="uif-col-6" style="padding-top:2%">
                    </div>

                </div>
                <div class="row">
                    <div class="uif-col-12">
                        <label>@Resources.Language.ProductProcessType :</label>
                    </div>
                </div>
                <div class="row">
                    <div class="uif-col-3">
                        <input type="checkbox" id="chkIsInteractive" name="IsInteractive" />
                        <label for="chkIsInteractive">@Resources.Language.ProductIsInteractive</label>
                    </div>
                    <div class="uif-col-3">
                        <input type="checkbox" id="chkIsCollective" name="IsCollective" disabled="disabled" />
                        <label for="chkIsCollective">@Resources.Language.ProductIsCollective</label>
                    </div>
                    <div class="uif-col-3">
                        <input type="checkbox" id="chkIsMassive" name="IsMassive" disabled="disabled" />
                        <label for="chkIsMassive">@Resources.Language.ProductIsMassive</label>
                    </div>
                </div>

            </div>
            <div class="uif-col-3 col-space-adjusted">
                <div class="uif-right-bar" id="panelButtonsRight">
                    <ul>
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0584"))
                        {
                            <li>
                                <a href="#" id="btnPaymentPlan">@Resources.Language.LabelPaymentPlans</a>
                                <p id="selectedPaymentPlan"></p>
                            </li>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0588"))
                        {
                            <li>
                                <a href="#" id="btnCoverage">@Resources.Language.LabelObjectsAndCoverages</a>
                                <p id="selectedCoverage"></p>
                            </li>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0595"))
                        {
                            <li>
                                <a href="#" id="btnRulesAndscripts">@Resources.Language.LabelRulesAndscripts</a>
                                <p id="selectedRulesAndscripts"></p>
                            </li>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0613"))
                        {
                            <li>
                                <a href="#" id="btnAdditionalData">@Resources.Language.LabelAdditionalData</a>
                                <p id="selectedAdditionalData"></p>
                            </li>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0603"))
                        {
                            <li>
                                <a href="#" id="btnAgents">@Resources.Language.LabelAgentsAndCommissions</a>
                                <p id="selectedAgents"></p>
                            </li>
                        }
                        @if (Sistran.Core.Framework.UIF.Web.Helpers.SessionHelper.IsAuthorize("0614"))
                        {
                            <li>
                                <a href="#" id="btnCopyProduct">@Resources.Language.LabelCopyProduct</a>
                                <p id="selectedCopyProduct"></p>
                            </li>
                        }
                        </ul>
                </div>
            </div>
        </div>
    </div>
</form>
