
@using Sistran.Core.Framework.UIF.Web.Resources
@model Sistran.Core.Application.GeneralLedgerServices.DTOs.AnalysisConceptKeyDTO

<div class="uif-panel">
    <div class="uif-row">
        <div class="uif-col-6">
            <label>@Global.TableName</label>
            @Html.TextBox("TableName", "", new { id = "TableName", data_val = "true", @onblur = " this.value=this.value.toUpperCase();", data_val_required = @Global.RequiredField, style = "text-align: left" })
            <span class="text-danger" data-valmsg-for="TableName" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-6">
            <label>@Global.ColumnName</label>
            @Html.TextBox("ColumnName", "", new { id = "ColumnName", data_val = "true", @onblur = " this.value=this.value.toUpperCase();", data_val_required = @Global.RequiredField, style = "text-align: left" })
            <span class="text-danger" data-valmsg-for="ColumnName" data-valmsg-replace="true" />
        </div>

    </div>

    <div class="uif-row" id="divBankAccount">
        <div class="uif-col-12">
            <label>@Global.Description</label>
            @Html.TextBox("Description", "", new { id = "Description", data_val = "true", data_val_required = @Global.RequiredField, style = "text-align: left" })
            <span class="text-danger" data-valmsg-for="Description" data-valmsg-replace="true" />
        </div>
    </div>

    <div class="uif-row">
        <div class="uif-col-12">
            <div class="uif-alert" id="alertForm"></div>
        </div>
    </div>
</div>
