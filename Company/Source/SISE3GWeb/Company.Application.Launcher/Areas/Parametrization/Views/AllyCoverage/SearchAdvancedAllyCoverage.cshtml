@model Sistran.Core.Framework.UIF.Web.Areas.Parametrization.Models.AllyCoverageViewModel
<script id="templateSearchAdvancedAllyCoverage" type="text/template">
    <div>
        <strong>Cobertura Id: </strong> {{CoverageId}}
    </div>
    <div>
        <strong>Cobertura Aliada Id: </strong> {{AllyCoverageId}} <strong>-Desc.: </strong> {{AllyCoverageId_object.PrintDescription}}
    </div>
    <div>
        <strong>Porcentaje: </strong> {{CoveragePct}}
    </div>
</script>
<form class="form" id="frm-adv-allycoverage">
    <div class="row">
        <div class="uif-col-12">
            <h4>Búsqueda avanzada</h4>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <label>Cobertura principal</label>
            <select class="uif-select form-control" id="ddlAdvPrincipalCov" data-id="Id" data-native="false" data-name="PrintDescription" data-val="true"></select>
        </div>
        <div class="uif-col-12">
            <label>Coberturas</label>
            <select class="uif-select form-control" id="ddlAdvCoverages" data-id="Id" data-native="false" data-name="PrintDescription" data-val="true"></select>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <button class="btn btn-primary-Co width-container" id="btnSearchAllyCoverage" type="button"> @Resources.Language.search</button>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="uif-listview" id="lvSearchAdvancedAllyCoverage">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="pull-right width-save">
                <button class="btn btn-default" id="btnCancelSearchAllyCoverage" type="button">@Resources.Language.Close</button>
                <button class="btn btn-primary" id="btnAcceptAdvAllyCoverage" type="button">@Resources.Language.Accept</button>
            </div>
        </div>
    </div>
</form>