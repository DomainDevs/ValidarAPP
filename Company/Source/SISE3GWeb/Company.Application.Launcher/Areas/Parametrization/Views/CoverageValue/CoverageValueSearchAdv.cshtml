@model Sistran.Core.Framework.UIF.Web.Areas.Parametrization.Models.CoverageValueViewModel


<script id="templateSearchAdvancedCoCoverage" type="text/template">
    

        <div>
            <strong>Ramo: </strong> {{Prefix.Description}}
        </div>
        <div>
            <strong>Cobertura: </strong> {{Coverage.Description}}
        </div>
        <div>
            <strong>Valor: </strong> {{Porcentage}}
        </div>

    
</script>

<form class="form" id="frmAdvCoCoverageValue">
    <div class="row">
        <div class="uif-col-12">
            <h4>@Resources.Language.AdvancedSearch</h4>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <label class="field-required">@Resources.Language.LabelPrefix</label>
            <select class="uif-select form-control" id="ddlPrefixSearch" data-id="Id" data-name="DescriptionLong" name="Prefix"
                    data-val="true" data-filter="true" data-native="false" data-val-required="@Resources.Language.ErrorfieldCountries"></select>

        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <label class="field-required">@Resources.Language.LabelCoverage</label>
            <select class="uif-select form-control" id="ddlCoverageSearch" data-id="Id" data-name="Description" name="Coverage"
                    data-val="true" data-filter="true" data-native="false" data-val-required="@Resources.Language.ErrorfieldState"></select>

        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <button class="btn btn-primary-Co width-container" id="btnSearchAdvAccept" type="button"> @Resources.Language.search</button>
        </div>
    </div>    
    <div class="row">
        <div class="uif-col-12">
            <div class="uif-listview" id="lvSearchAdvanced">
            </div>
        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <div class="pull-right width-save">
                <button class="btn btn-default" id="btnCancelAdv" type="button">@Resources.Language.Close</button>
                <button class="btn btn-primary" id="btnOkSearchAdvCoverage" type="button">@Resources.Language.Accept</button>
                                                    
            </div>
        </div>
    </div>
</form>


