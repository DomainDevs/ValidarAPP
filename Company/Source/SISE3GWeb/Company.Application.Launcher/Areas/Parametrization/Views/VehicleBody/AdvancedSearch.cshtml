        <div class="uif-col-12">
            <div class="uif-listview" id="listViewSearchAdv">
            </div>
        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <div class="pull-right">
                <button class="btn btn-default" id="btnCancelVehicleBodyAdv" type="button" >@Resources.Language.Close</button>
                <button class="btn btn-primary" id="btnOkSearchVehicleBodyAdv" type="button" >@Resources.Language.Accept</button>
            </div>
        </div>
    </div>
