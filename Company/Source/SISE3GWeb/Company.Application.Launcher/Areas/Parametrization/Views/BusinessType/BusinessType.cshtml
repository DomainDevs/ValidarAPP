@model Sistran.Core.Framework.UIF.Web.Areas.Parametrization.Models.BusinessTypeViewModel
@{
    Layout = null;
}
<script id="BusinessTypeTemplate" type="text/template">
    <div><br />Tipo de Negocios:<strong> {{SMALL_DESCRIPTION}} ({{BUSINESS_TYPE_CD}})</strong></div>
</script>

<script id="title-template" type="text/template">
    <div style="width:100%">
        @Resources.Language.LabelTabBusinessType
    </div>
</script>

<script id="footer-template" type="text/template">
    <div id="butt" class="footerModule">
        <button class="btn btn-default cancel" id="btnExit">
            <i class="fa fa-times-circle" aria-hidden="true"></i>
            <span class="" aria-hidden="true"></span>
            @Resources.Language.ButtonExit
        </button>
        <button class="btn btn-default" id="btnExport">
            <i class="fa fa-cloud-download" aria-hidden="true"></i>
            <span class="" aria-hidden="true"></span>
            @Resources.Language.ButtonExport
        </button>
    </div>
</script>

<form id="formBusinessType" autocomplete="off" class="form">
    <div class="row">
        <input type="hidden" id="inputId" />
        <div class="uif-col-6  fit-col-secondary center-block" id="panelBusinessType">
            <div id="listBusinessType" class="uif-listview"></div>
        </div>
    </div>
</form>
