@model Sistran.Core.Framework.UIF.Web.Areas.Parametrization.Models.BillingPeriodViewModel

@{
    Layout = null;
}

<script id="BillingPeriodTemplate" type="text/template">
    <div><br />@Resources.Language.LabelTabBillingPeriod:<strong> {{DESCRIPTION}} ({{BILLING_PERIOD_CD}})</strong></div>
</script>

 <script id="title-template" type="text/template">
        <div style="width:100%">
            @Resources.Language.LabelTabBillingPeriod
        </div>
  </script>

 <script id="footer-template" type="text/template">
     <div id="butt" class="footerModule">
         <button class="btn btn-default cancel" id="btnExit">
             <i class="fa fa-times-circle" aria-hidden="true"></i>
             <span class="" aria-hidden="true"></span>
             @Resources.Language.ButtonExit
         </button>
         <button class="btn btn-default" id="btnExport">
             <i class="fa fa-cloud-download" aria-hidden="true"></i>
             <span class="" aria-hidden="true"></span>
             @Resources.Language.ButtonExport
         </button>
     </div>
 </script>



<form id="formBillingPeriod" autocomplete="off" class="form">
    <div class="row">
        <input type="hidden" id="inputId" />
        <div class="uif-col-8  fit-col-secondary center-block" id="panelBillingPeriod">
            <div id="listBillingPeriod" class="uif-listview">
            </div>
        </div>
    </div>
</form>

