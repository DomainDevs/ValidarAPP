@{
    Layout = null;
}
<script id="VehicleTypeTemplateAdv" type="text/template">
    <label>@Resources.Language.VehicleType :({{TypeCode}}) {{ShortDescription}} {{Description}} </label>
    {{#if IsTruck}}
    <br />
    <label>Es Camión</label>
    {{/if}}
    {{#if IsActive}}
    <br />
    <label>Activo</label>
    {{/if}}
</script>

<form class="form">
    <div class="row">
        <div class="uif-col-12">
            <div class="uif-listview" id="listViewSearchAdv">
            </div>
        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <div class="pull-right">
                <button class="btn btn-default" id="btnCancelVehicleTypeAdv" type="button" >@Resources.Language.Close</button>
                <button class="btn btn-primary" id="btnOkSearchVehicleTypeAdv" type="button" >@Resources.Language.Accept</button>
            </div>
        </div>
    </div>
</form>
