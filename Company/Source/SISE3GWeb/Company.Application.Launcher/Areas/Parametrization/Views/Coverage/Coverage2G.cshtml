<form id="formCoverage2G" class="form">
	<div class="uif-row" id="mainModelCoverage2G">
		<div class="uif-col-6">
			<label class="field-required">@Resources.Language.LabelInsuranceObject2G</label>
			<select class="uif-select form-control" id="selectInsuranceObject2G" data-val="true"
					data-id="Id" data-name="Id" name="InsuredObjectId2G"
