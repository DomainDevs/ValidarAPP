@model Sistran.Core.Framework.UIF.Web.Areas.Parametrization.Models.RateTaxAdvancedSearchViewModel
<script id="AdvSearchRateTaxTemplate" type="text/template">
</script>

<form class="form" id="formAdvSearchRateTax">
    <div class="row">
        <div class="uif-col-12">
            <h4>@Resources.Language.AdvancedSearchTaxRates</h4>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-4">
            @Html.LabelFor(model => model.Condition, htmlAttributes: new { @class = "control-label field-required" })
            <select data-filter="true" data-native="false" class="uif-select form-control" id="Condition" name="Condition"
                    data-id="Id" data-name="Description"
                    data-val="true"
                    data-val-required=""></select>
            <span class="text-danger" data-valmsg-for="Condition" data-valmsg-replace="true"></span>
        </div>
        <div class="uif-col-4">
            @Html.LabelFor(model => model.CategoryAdvanced, htmlAttributes: new { @class = "control-label field-required" })
            <select data-filter="true" data-native="false" class="uif-select form-control" id="CategoryAdvanced" name="CategoryAdvanced"
                    data-id="Id" data-name="Description"
                    data-val="true"
                    data-val-required=""></select>
            <span class="text-danger" data-valmsg-for="CategoryAdvanced" data-valmsg-replace="true"></span>
        </div>
        <div class="uif-col-4">
            @Html.LabelFor(model => model.Town, htmlAttributes: new { @class = "control-label field-required" })
            <select data-filter="true" data-native="false" class="uif-select form-control" id="Town" name="Town"
                    data-id="Id" data-name="Town"
                    data-val="true"
                    data-val-required=""></select>
            <span class="text-danger" data-valmsg-for="Town" data-valmsg-replace="true"></span>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-4">

        </div>
        <div class="uif-col-4">
            @Html.LabelFor(model => model.Office, htmlAttributes: new { @class = "control-label field-required" })
            <select data-filter="true" data-native="false" class="uif-select form-control" id="Office" name="Office"
                    data-id="Id" data-name="Description"
                    data-val="true"
                    data-val-required=""></select>
            <span class="text-danger" data-valmsg-for="Office" data-valmsg-replace="true"></span>
        </div>
        <div class="uif-col-4">
            @Html.LabelFor(model => model.TechnicalBranch, htmlAttributes: new { @class = "control-label field-required" })
            <select data-filter="true" data-native="false" class="uif-select form-control" id="TechnicalBranch" name="TechnicalBranch"
                    data-id="Id" data-name="TechnicalBranch"
                    data-val="true"
                    data-val-required=""></select>
            <span class="text-danger" data-valmsg-for="TechnicalBranch" data-valmsg-replace="true"></span>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-8">
            @Html.LabelFor(model => model.Coverages, htmlAttributes: new { @class = "control-label field-required" })
            <select data-filter="true" data-native="false" class="uif-select form-control" id="Coverages" name="Coverages"
                    data-id="Id" data-name="Description"
                    data-val="true"
                    data-val-required=""></select>
            <span class="text-danger" data-valmsg-for="Coverages" data-valmsg-replace="true"></span>
        </div>
        <div class="uif-col-4">
            <label>@string.Empty</label>
            <div class="row-space-adjusted">
                <input type="checkbox" id="Enabled" name="Enabled" />
                @Html.LabelFor(model => model.Enabled, htmlAttributes: new { @class = "control-label" })
            </div>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="without-label">
                <button class="btn btn-primary-Co width-container" id="SearchAdvancedRateTax" type="button"> @Resources.Language.search</button>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="uif-listview" id="lvSearchAdvRateTax">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="pull-right width-save">
                <button class="btn btn-default" id="btnCancelSearchAdvVehicle" type="button">@Resources.Language.Close</button>
                <button class="btn btn-primary" id="btnOkSearchAdvVehicle" type="button">@Resources.Language.Accept</button>
            </div>
        </div>
    </div>
</form>


