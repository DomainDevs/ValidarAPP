@model Sistran.Core.Framework.UIF.Web.Areas.Massive.Models.MassiveViewModel
@{
    Layout = null;
}
<script id="title-template" type="text/template">
    <div class="column-70">
        @Resources.Language.LoadFasecolda
    </div>
    <div class="column-30 even">
        <input class="uif-input-search" type="text" name="inputTemporalId" data-val="true" data-id="Id" id="inputTemporal" placeholder="Buscar cargue">
    </div>
</script>
<script id="footer-template" type="text/template">
    <div id="buttonsFaseColda" class="footerFaseColda">
        <button type="button" class="btn btn-default" id="btnExit">
            <i class="fa fa-times-circle" aria-hidden="true"></i>
            <span class="" aria-hidden="true"></span>@Resources.Language.ButtonExit
        </button>
        <button type="button" class="btn btn-default" id="btnErrors">
            <i class="fa fa-cloud-download" aria-hidden="true"></i>
            <span class="" aria-hidden="true"></span>@Resources.Language.Error
        </button>
        <button type="button" class="btn btn-default" id="btnNew">
            <i class="fa fa-file-o" aria-hidden="true"></i>
            <span class="" aria-hidden="true"></span>@Resources.Language.ButtonNew
        </button>
        <button type="button" class="btn btn-primary" id="btnLoad">@Resources.Language.Load</button>
        <button type="button" class="btn btn-default" id="btnProcess">
            <i class="fa fa-floppy-o" aria-hidden="true"></i>
            <span class="" aria-hidden="true"></span>@Resources.Language.ButtonGenerateProcess
        </button>
    </div>
</script>

<div id="uif-spinner-loading" hidden="hidden">
    <div class="spinner spinner-three-bounce">
        <div class="bounce1"></div>
        <div class="bounce2"></div>
        <div class="bounce3"></div>
    </div>
</div>

<form id="formVehicleFasecolda" autocomplete="off" class="form">
    <div class="row">
        <div class="uif-col-12 fit-col-main">
            <div class="uif-row">
                <div class="uif-col-9">
                    <label class="field-required">Archivo de Códigos Fasecolda</label>
                    <input class="text" type="text" name="inputCodigoFaseColda" data-val="true" data-id="Id" id="inputCodigoFaseColda" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                    <span class="text-danger" data-valmsg-for="inputCodigoFaseColda" data-valmsg-replace="true"></span>
                </div>
                <div class="uif-col-3">
                    <label class="field-required">@Resources.Language.LabelTextFile</label>
                    @Html.TextBoxFor(model => model.FileName,
                        new Dictionary<string, Object>
                        {
                            {"input,","false"},
                            { "id","inputCodigoFile"},
                            { "class", "inputfile"},
                            { "type","file"},
                            {"data-text",Resources.Language.ChooseFile},
                            {"accept",".txt"}
                        })
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12 withOutMargin">
                    <div class="progress" style="height: 35px">
                        <div id="progress-bar-code" class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">
                            <span class="sr-only">0% complete</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-9">
                    <label class="field-required">Archivo de Valores Fasecolda</label>
                    <input class="text" type="text" name="inputValueFaseColdaId" data-val="true" data-id="Id" id="inputValueFaseColda" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                    <span class="text-danger" data-valmsg-for="inputValueFaseColdaId" data-valmsg-replace="true"></span>
                </div>
                <div class="uif-col-3">
                    <label class="field-required">@Resources.Language.LabelTextFile</label>
                    @Html.TextBoxFor(model => model.FileName,
                        new Dictionary<string, Object>
                        {
                            {"input,","false"},
                            { "id","inputValueFile"},
                            { "class", "inputfile"},
                            { "type","file"},
                            {"data-text",Resources.Language.ChooseFile},
                            {"accept",".txt"}
                        })
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12 withOutMargin">
                    <div class="progress" style="height: 35px">
                        <div id="progress-bar-value" class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">
                            <span class="sr-only">0% complete</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12">
                    <div class="" style="float: right">
                        <button type="button" class="btn btn-default secondary-Colombia" id="btnAddFiles">
                            @Resources.Language.ButtonLoad
                        </button>
                    </div>
                </div>
            </div>
            <div id="wizardCodes" class="uif-wizard">
                <div class="uif-step" id="step1" data-title="@Resources.Language.LabelChargue - Codigos">
                    <span id="stepCodes1" style="margin-top: -15px;padding: 0px 0px 0px 45px;position: absolute; color:#09427c;">EnProceso</span>
                    <span id="stepCodes2" style="margin-top: -15px;padding: 0px 0px 0px 45px;position: absolute; color:#09427c;">Finalizado</span>
                </div>
            </div>
            <div class="uif-row" id="stepsFasecoldaCodes">
                <div class="uif-col-3">
                    <label>Total Registros</label>
                    <input class="text" type="text" name="totalRecords" data-val="true" data-id="Id" id="inputCodesTotalRecords" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
                <div class="uif-col-3">
                    <label>Procesados</label>
                    <input class="text" type="text" name="processed" data-val="true" data-id="Id" id="inputCodesProcessed" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
                <div class="uif-col-3">
                    <label>Pendientes</label>
                    <input class="text" type="text" name="pending" data-val="true" data-id="Id" id="inputCodesPending" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
                <div class="uif-col-3">
                    <label>Con Errores</label>
                    <input class="text" type="text" name="errors" data-val="true" data-id="Id" id="inputCodesErrors" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
            </div>
            <div id="wizardValues" class="uif-wizard">
                <div class="uif-step" id="step2" data-title="@Resources.Language.LabelChargue - Valores">
                    <span id="stepValues1" style="margin-top: -15px;padding: 0px 0px 0px 45px;position: absolute; color:#09427c;">EnProceso</span>
                    <span id="stepValues2" style="margin-top: -15px;padding: 0px 0px 0px 45px;position: absolute; color:#09427c;">Finalizado</span>
                </div>
            </div>
            <div class="uif-row" id="stepsFasecoldaValues">
                <div class="uif-col-3">
                    <label>Total Registros</label>
                    <input class="text" type="text" name="totalRecords" data-val="true" data-id="Id" id="inputValuesTotalRecords" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
                <div class="uif-col-3">
                    <label>Procesados</label>
                    <input class="text" type="text" name="processed" data-val="true" data-id="Id" id="inputValuesProcessed" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
                <div class="uif-col-3">
                    <label>Pendientes</label>
                    <input class="text" type="text" name="pending" data-val="true" data-id="Id" id="inputValuesPending" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
                <div class="uif-col-3">
                    <label>Con Errores</label>
                    <input class="text" type="text" name="errors" data-val="true" data-id="Id" id="inputValuesErrors" data-val-required="@Resources.Language.ErrorEmptyRequiredFields" required disabled>
                </div>
            </div>
        </div>
    </div>
</form>