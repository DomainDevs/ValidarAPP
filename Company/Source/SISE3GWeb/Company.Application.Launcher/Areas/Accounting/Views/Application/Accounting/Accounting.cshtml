@using Sistran.Core.Framework.UIF.Web.Resources

<script id="AnalysisCodesList-display-template" type="text/template">
    <div>
        <strong>@Global.AnalysisCode:</strong>  {{AnalysisId}}<br />
        <strong>@Global.AnalysisConcept @Global.Code:</strong> {{AnalysisConcept.AnalysisConceptId}}<br>
        <strong>@Global.Description:</strong> {{Description}}<br>
        <strong>@Global.Key @Global.Concept:</strong> {{ConceptKey}}<br />
    </div>
</script>
<script id="CostCenterList-display-template" type="text/template">
    <div>
        <strong>@Global.CostCenter:</strong>  {{CostCenter.CostCenterId}}<br />
        <strong>@Global.CostCenter:</strong>  {{CostCenter.Description}}<br />
        <strong>@Global.Percentage:</strong> {{Percentage}}<br>
    </div>
</script>

<script id="accountingMovementsList-display-template" type="text/template">
    <strong>@Global.Branch:</strong>  {{Branch.Description}}
    <br>
    <strong>@Global.SalePoint:</strong> {{SalePoint.Description}}
    <strong>@Global.Concept:</strong> {{AccountingConcept.Description}}
    <br>
    <strong>@Global.AccountingAccount:</strong> {{BookAccount.AccountNumber}}
    <br>
    <strong>@Global.Nature:</strong> {{AccountingNatureDescription}} |
    <strong>@Global.Currency:</strong> {{Amount.Currency.Description}}
    <br>
    <div class="number">
        <strong>@Global.ExchangeRate:</strong> {{ExchangeRate.SellAmount}} | <strong>@Global.Amount:</strong> {{FormatAmount}}
    </div>
    <strong>@Global.Description:</strong> {{Description}}
    <strong>@Global.AnalysisCode: </strong>
    {{#ifCond ItHasCodeAnalysis "==" true }}
    @Global.Yes
    {{else ifCond ItHasCodeAnalysis "==" false}}
    @Global.No
    {{/ifCond}}
    <br />
    <strong>@Global.CostCenter: </strong>
    {{#ifCond ItHasCostCenter "==" true }}
    @Global.Yes
    {{else ifCond ItHasCostCenter "==" false}}
    @Global.No
    {{/ifCond}}
    <br />

</script>

