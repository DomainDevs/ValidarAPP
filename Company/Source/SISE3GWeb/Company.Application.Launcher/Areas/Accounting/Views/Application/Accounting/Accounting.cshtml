                                <select class="uif-select form-control"
                                        id="AccountingCostCenter"
                                        data-id="CostCenterId"
                                        data-name="Description"></select>
                                <span class="text-danger" data-valmsg-for="AccountingCostCenter" data-valmsg-replace="true" />
                            </div>
                        </div>
                        <div class="uif-row">
                            <div class="uif-col-12">
                                <label>@Global.Percentage</label>
                                @Html.TextBox("AccountingPercentageAmount", "",
                                          new
                                          {
                                              id = "AccountingPercentageAmount",
                                              data_val = "true",
                                              data_val_required = @Global.RequiredField,
                                              @onkeypress = "return OnlyDecimalNumber(event,this)",
                                              maxlength = "3"
                                          })
                            </div>
                        </div>
                        <div class="uif-row">
                            <div class="uif-col-12">
                                <div class="uif-alert" id="costCenterAlert"></div>
                            </div>
                        </div>

                        <div class="uif-row">
                            <div class="uif-col-12">
                                <div class="pull-right">
                                    <button type="submit" class="btn btn-primary" id="AddAccountingCostCenterMovement" onclick=" return false ">@Global.Add</button>
                                </div>
                            </div>
                        </div>
                        <div class="uif-row">
                            <div class="uif-col-12">
                                <div id="CostCenterList" class="uif-listview">
                                    <div class="footer">
                                        <label>@Global.Total @Global.Percentage:</label>
                                        <b id="TotalPercentage">0.00</b>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>


            </div>

        </div>
        @*Right Column*@
        <div class="uif-col-6">
            <div class="uif-panel" data-title="@Global.AccountingMovements">
                <div id="accountingMovementsList" class="uif-listview">
                    <div class="footer">
                        <label>@Global.Credit</label>
                        <b id="TotalAccountingMovement">$0.00</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        <label>@Global.Debit</label>
                        <b id="TotalDebitAccountingMovement">$0.00</b>
                    </div>
                </div>
            </div>
        </div>
        @*End Right Column*@
    </div>
    <div class="uif-row">
        <div class="pull-right">
            <div class="uif-modalbuttons">
                <button type="button" class="btn btn-default" id="AccountingAcceptMovement">@Global.Accept</button>
            </div>
        </div>
    </div>
</div>

