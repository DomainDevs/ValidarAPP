                   onkeypress="return JustNumbers(event,this)"
                   data-val="true" class="required" />
            <span class="text-danger" data-valmsg-for="TransferDocumentNumber" data-valmsg-replace="true" />
        </div>
    </div>

    <div class="uif-row">
        <div class="uif-col-6">
            <label class="control-label">
                @Global.ReceivingBank
            </label>
            <br />
            <select class="uif-select form-control required" id="TransferReceivingBankSelect" name="TransferReceivingBankSelect"
                    data-source=@Url.Content("~/Accounting/Common/GetAccountBanks")
                    data-id="Id" data-native="true"
                    data-name="Description" data-val="true"
                    data-val-required='@Global.RequiredField'></select>
            <span class="text-danger" data-valmsg-for="TransferReceivingBankSelect" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-6">
            <label class="control-label">
                @Global.ReceivingBankAccountNumber
            </label>
            <br />
            <select class="uif-select form-control required" id="AccountNumberReceivingBankSelect" name="AccountNumberReceivingBankSelect"
                    data-source='' data-id="Id"
                    data-name="Description" data-val="true"
                    data-val-required='@Global.RequiredField'></select>
