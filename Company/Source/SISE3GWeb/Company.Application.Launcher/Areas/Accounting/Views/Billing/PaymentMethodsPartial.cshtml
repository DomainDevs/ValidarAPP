@using Sistran.Core.Framework.UIF.Web.Resources


<div class="uif-row">
    <div class="uif-col-3">   
        <select class="uif-select form-control" id="PaymentMethodType" name="PaymentMethodType"
                data-source='@Url.Content("~/Regularization/GetEnabledPaymentMethodTypess")' data-id="Id"
                data-name="Description" data-val="true" data-native="true"
                data-val-required='@Global.RequiredField'></select>
        <span class="text-danger" data-valmsg-for="PaymentMethodType" data-valmsg-replace="true" />
    </div>
</div>


<!-- Cash -->
<form id="divCash">
    <div class="uif-row">
        <div class="uif-col-3">
            <label>@Global.Currency</label>
            <select class="uif-select form-control required" id="CashCurrencySelect" name="CashCurrencySelect"
                    data-source='@Url.Content("~/Accounting/Common/GetCurrencies")' data-id="Id"
                    data-name="Description" data-val="true" data-native="true"
                    data-val-required='@Global.RequiredField'></select>
            <span class="text-danger" data-valmsg-for="CashCurrencySelect" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-3">
            <label>@Global.Amount</label>
            <br />
            @*<input type="text" id="CashAmount" name="CashAmount" style="text-align: right">*@
            <input type="text" id="CashAmountRegularization" name="CashAmountRegularization" style="text-align: right"
                   class="required" onkeypress="return OnlyDecimalNumber(event,this)" data-val="true"
                   data-val-required='@Global.RequiredField' />
            <span class="text-danger" data-valmsg-for="CashAmountRegularization" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-3">
            <label>@Global.ExchangeRate</label>
            <br/>
            <input type="text" id="CashExchangeRate" disabled="disabled" style="text-align: right"/>
        </div>
        <div class="uif-row">
            <div class="uif-col-3">
                <label>@Global.LocalAmount</label>
                <br />
                <input type="text" id="CashLocalAmount" disabled="disabled" style="text-align: right" />
            </div>
        </div>
    </div>
</form>
<!-- End Cash -->
<!-- Check -->
<form id="divCheck">
    <div class="uif-row">
        <div class="uif-col-6">
            <label>@Global.Currency</label>
            <select class="uif-select form-control" id="CheckCurrencySelect" name="CheckCurrencySelect"
                    data-source='@Url.Content("~/Accounting/Common/GetCurrencies")' data-id="Id"
                    data-name="Description" data-val="true" data-native="true"
                    data-val-required='@Global.RequiredField'></select>
            <span class="text-danger" data-valmsg-for="CheckCurrencySelect" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-3">
            <label>@Global.Amount</label>
            <br />
            <input type="text" id="CheckAmount" style="text-align: right" name="CheckAmount"
                   onkeypress="return OnlyDecimalNumber(event,this)" data-val="true"
                   data-val-required='@Global.RequiredField' />
            <span class="text-danger" data-valmsg-for="CheckAmount" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-3">
            <label>@Global.ExchangeRate</label>
            <br />
            <input type="text" id="CheckExchangeRate" disabled="disabled" style="text-align: right" />
        </div>
    </div>
    <div class="uif-row">
        <div class="uif-col-3">
            <label>@Global.LocalAmount</label>
            <br />
            <input type="text" id="CheckLocalAmount" disabled="disabled" style="text-align: right" />
        </div>
        <div class="uif-col-6">
            <label>@Global.IssuingBank</label>
            <br />
            <input class="uif-autocomplete" type="text" data-source="@Url.Action("GetBanks", "Billing")" data-displaykey="Value"
                   placeholder='@Global.EnterIssuingBank' id="CheckBank" name="CheckBank" data-val="true"
                   data-val-required='@Global.RequiredField'>
            <span class="text-danger" data-valmsg-for="CheckBank" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-3">
            <label>@Global.AccountNumber</label>
            <br />
            <input type="text" id="CheckAccountNumber" style="text-align: right" maxlength="15" onkeypress=" return JustNumbers(event, this); "
                   name="CheckAccountNumber" data-val="true"
                   data-val-required='@Global.RequiredField' />
            <span class="text-danger" data-valmsg-for="CheckAccountNumber" data-valmsg-replace="true" />
        </div>
    </div>
    <div class="uif-row">
        <div class="uif-col-3">
            <label>@Global.DocumentNumber</label>
            <br />
            <input type="text" id="CheckDocumentNumber" style="text-align: right" maxlength="15" onkeypress=" return JustNumbers(event, this); "
                   name="CheckDocumentNumber" data-val="true"
                   data-val-required='@Global.RequiredField' />
            <span class="text-danger" data-valmsg-for="CheckDocumentNumber" data-valmsg-replace="true" />
        </div>
        <div class="uif-col-6">
            <label>@Global.PayHolderName</label>
            <br />
            <input type="text" id="CheckHolderName" />
        </div>
        <div class="uif-col-3">
            <label>@Global.Date</label>
            <br />
            <input type="text" value="@ViewBag.AccountingDate" class="uif-datepicker" data-mask="99/99/9999" id="CheckDateRegularizated"
                   name="CheckDateRegularizated" data-min-date="@ViewBag.CurrentDate" data-max-date="@ViewBag.CurrentDate"
                   data-val="true" data-val-required='@Global.RequiredField' />
            <input type="text" id="CheckPostDate" name="CheckPostDate" 
                   data-min-date="@ViewBag.CurrentDate" data-max-date="@ViewBag.CurrentDate"
                    class="uif-datepicker" data-val-required='@Global.RequiredField' data-mask="99/99/9999" />
            <span class="text-danger" data-valmsg-for="CheckDateRegularizated" data-valmsg-replace="true" />
            <span class="text-danger" data-valmsg-for="CheckPostDate" data-valmsg-replace="true" />
        </div>
    </div>
</form>
<!-- End Check -->
<!-- Credit Card -->
<form id="divCreditCard">
    <div class="uif-row">
        <div class="uif-col-6">
            <label>@Global.Currency</label>
            <select class="uif-select form-control" id="CreditCardCurrencySelect" name="CreditCardCurrencySelect"
                    data-source='@Url.Content("~/Accounting/Common/GetCurrencies")' data-id="Id" data-native="true"
                    data-name="Description"></select>
        </div>
        <div class="uif-col-3">
            <label>@Global.Amount</label>
            <br />
            <input type="text" id="CreditCardAmount" style="text-align: right" onkeypress="return OnlyDecimalNumber(event,this)" />
        </div>
        <div class="uif-col-3">
            <label>@Global.Exchange</label>
            <br />
            <input type="text" id="CreditCardExchangeRate" disabled="disabled" style="text-align: right" />
        </div>
    </div>
    <div class="uif-row">
        <div class="uif-col-3">
            <label>@Global.LocalAmount</label>
            <br />
            <input type="text" id="CreditCardLocalAmount" disabled="disabled" style="text-align: right" />
        </div>
        <div class="uif-col-9">
            <label>@Global.Conduit</label>
            <br />
            <select class="uif-select form-control" id="CreditCardTypeSelect" name="CreditCardTypeSelect"
                    data-source='@Url.Content("~/Accounting/Common/GetCreditCardTypes")' data-id="Id" data-native="true"
                    data-name="Description"></select>
        </div>
    </div>
    <div class="uif-row">
        <div class="uif-col-6">
            <label>@Global.IssuingBank</label>
            <br />
            <input class="uif-autocomplete" type="text" data-source="@Url.Action("GetBanks", "Billing")" data-displaykey="Value" placeholder='@Global.EnterIssuingBank' id="CreditCardIssuingBankInput">
        </div>
        <div class="uif-col-3">
            <label>@Global.CreditCardNumber</label>
            <br />
            <input type="text" id="CreditCardNumber" style="text-align: right" maxlength="16" />
        </div>
        <div class="uif-col-3">
            <label>@Global.AuthorizationNumber</label>
            <br />
            <input type="text" id="CreditCardAuthorizationNumber" style="text-align: right" maxlength="12" />
        </div>
    </div>
    <div class="uif-row">
        <div class="uif-col-3">
            <label>@Global.VoucherNumber</label>
            <br />
            <input type="text" id="CreditCardVoucherNumber" name="CreditCardVoucherNumber" style="text-align: right" maxlength="12" />
        </div>
        <div class="uif-col-9">
            <label>@Global.PayCreditCardName</label>
            <br />
            <input type="text" id="CreditCardHolderName" name="CreditCardHolderName" />
        </div>
    </div>
    <div class="uif-row">
        <div class="uif-col-9">
            <label>@Global.ExpirationMonthYear</label>
            <br />
            <div class="uif-row">
                <div class="uif-col-6">
                    <select class="uif-select form-control" id="CreditCardValidThruMonthSelect" name="selectCreditCardValidThruMonth"
                            data-source='@Url.Content("~/Accounting/Common/GetValidMonths")' data-id="Id" data-name="Description" data-native="true"></select>
                </div>
                <div class="uif-col-6">
                    <input type="text" id="CreditCardValidThruYear" name="CreditCardValidThruYear" data-pattern="0000" class="uif-mask" />
                </div>
            </div>
        </div>
        <div class="uif-col-3">
            <div class="uif-row">
                <div class="uif-col-12">
                    <label>@Global.TaxBase</label>
                    <br />
                    <input type="text" id="CreditCardTaxBase" style="text-align: right" onkeypress="return OnlyDecimalNumber(event,this)" />
                </div>
            </div>
        </div>
    </div>
</form>
<!-- End CreditCard -->


<form id="divTransfer">
