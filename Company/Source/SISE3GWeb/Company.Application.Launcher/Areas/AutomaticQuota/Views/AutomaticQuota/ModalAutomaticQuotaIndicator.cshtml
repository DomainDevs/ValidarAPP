<form id="formAutomaticQuotaIndicator" class="form">

    <div class="uif-modalbuttons width-save">
        <button type="button" class="btn btn-primary" id="btnIndicatorSave" data-dismiss="modal">@Resources.Language.ButtonSave</button>
        <button type="button" class="btn btn-default" id="btnIndicatorClose" data-dismiss="modal">@Resources.Language.ButtonClose</button>
    </div>

    <div class="uif-row" id="EditTableIndicator">
        <div class="uif-col-4">
            <label class="field-required">@Resources.Language.Concept</label>
            <h4><span id="InputIndicatorConcept"> </span></h4>
        </div>
        <div class="uif-col-4">
            <label class="field-required">@Resources.Language.Observations</label>
            <input type="text" id="InputObservations" maxlength="20" />
        </div>
        <div class="uif-col-4">
            <button type="button" class="btn btn-default" id="btnEditIndicatorsave">@Resources.Language.ButtonSave</button>
        </div>
    </div>

    <div class="uif-row" id="ButtonEditTableIndicator">

    </div>

    <div class="row">
        <div class="uif-col-12">
            <table class="uif-table" id="tableLiquidity" data-title="@Resources.Language.LabelTheliquidity" data-filter="true" data-paginate="true" data-selectiontype="none" data-add="false" data-edit="true" data-delete="false">
                <thead>
                    <tr>
                        <th data-property="Id" data-visible="false"><b>@Resources.Language.LabelId</b></th>
                        <th data-property="Description"><b>@Resources.Language.Concepts</b></th>
                        <th data-property="DateIni" @*data-type="currency"*@><b><span id="DateIniLiquidity"></span></b></th>
                        <th data-property="DateFin" @*data-type="currency"*@><b><span id="DateFinLiquidity"></span></b></th>
                        <th data-property="Observations"><b>@Resources.Language.Observations</b></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <table class="uif-table" id="tableIndebtedness" data-title="@Resources.Language.LabelTheIndebtedness" data-filter="true" data-paginate="false" data-selectiontype="none" data-add="false" data-edit="true" data-delete="false">
                <thead>
                    <tr>
                        <th data-property="Description"><b>@Resources.Language.Concepts</b></th>
                        <th data-property="DateIni" @*data-type="currency"*@><b><span id="DateIniIndebtedness"></span></b></th>
                        <th data-property="DateFin" @*data-type="currency"*@><b><span id="DateFinIndebtedness"></span></b></th>
                        <th data-property="Observations"><b>@Resources.Language.Observations</b></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <table class="uif-table" id="tableCosteffectiveness" data-title="@Resources.Language.LabelTheCosteffectiveness" data-filter="true" data-paginate="false" data-selectiontype="none" data-add="false" data-edit="true" data-delete="false">
                <thead>
                    <tr>
                        <th data-property="Description"><b>@Resources.Language.Concepts</b></th>
                        <th data-property="DateIni" @*data-type="currency"*@><b><span id="DateIniCosteffectiveness"></span></b></th>
                        <th data-property="DateFin" @*data-type="currency"*@><b><span id="DateFinCosteffectiveness"></span></b></th>
                        <th data-property="Observations"><b>@Resources.Language.Observations</b></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>

    <div class="row">
        <div class="uif-col-12">
            <table class="uif-table" id="tableActivity" data-title="@Resources.Language.LabelTheActivity" data-filter="true" data-paginate="false" data-selectiontype="none" data-add="false" data-edit="true" data-delete="false">
                <thead>
                    <tr>
                        <th data-property="Description"><b>@Resources.Language.Concepts</b></th>
                        <th data-property="DateIni" @*data-type="currency"*@><b><span id="DateIniActivity"></span></b></th>
                        <th data-property="DateFin" @*data-type="currency"*@><b><span id="DateFinActivity"></span></b></th>
                        <th data-property="Observations"><b>@Resources.Language.Observations</b></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</form>