<div data-disabled="false" data-visible="" id="searchClaimTable">
    <div class="row">
        <div class="uif-col-12">
            <table id="tblClaims" class="uif-table" data-title="<b>@Resources.Language.ClaimListing</b>" data-selectiontype="single">
                <thead>
                    <tr>
                        <th data-property="Number"><b>@Resources.Language.ClaimNumberAbbreviation</b></th>
                        <th data-property="NoticeDate"><b>@Resources.Language.NoticeDate</b></th>
                        <th data-property="NoticeInternalConsecutive"><b>@Resources.Language.InternalConsecutive</b></th>
                        <th data-property="OccurrenceDate"><b>@Resources.Language.OcurrenceDate</b></th>
                        <th data-property="BranchDescription"><b>@Resources.Language.Branch</b></th>
                        <th data-property="PrefixDescription"><b>@Resources.Language.Prefix</b></th>
                        <th data-property="PolicyDocumentNumber"><b>@Resources.Language.Policy</b></th>
                        <th data-property="RiskDescription"><b>@Resources.Language.Risk</b></th>
                        <th data-property="Description"><b> @Resources.Language.LabelClaimDetail</b></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>

</div>