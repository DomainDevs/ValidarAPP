<div data-disabled="false" data-visible="" id="searchChargeTable">
    <div class="row">
        <div class="uif-col-12">
            <table id="tblCharge" class="uif-table" data-title="<b>@Resources.Language.ChargeRequestList</b>"
                   data-filter="false" data-paginate="true" data-selectiontype="single">
                <thead>
                    <tr>
                        <th data-property="ClaimNumber"><b>@Resources.Language.ComplaintNotice</b></th>
                        <th data-property="BranchDescription"><b>@Resources.Language.RecoveryBranch</b></th>
                        <th data-property="PrefixDescription"><b>@Resources.Language.Prefix</b></th>
                        <th data-property="Number"><b>@Resources.Language.PaymentRequestNumber</b></th>
                        <th data-type="currency" data-property="RecoveryOrSalvageAmount"><b>@Resources.Language.RecoveryOrSalvageAmount</b></th>
                        <th data-type="currency" data-property="TotalAmount"><b>@Resources.Language.RecoveryAmount</b></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
