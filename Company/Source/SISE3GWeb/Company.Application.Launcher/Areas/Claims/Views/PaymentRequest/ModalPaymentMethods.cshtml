<form id="formPaymentMethods" class="form">
    <div class="row">
        <div class="uif-col-12 fit-col-main">
            <div class="uif-row">
                <div class="uif-col-12">
                    <label>Cuenta Predeterminada</label>
                    <select class="uif-select form-control" id="selectTypeOfProof" data-val="true" data-source=''
                            data-val-required="Origen Requerido" data-id="Id" data-name="Description" name="BranchId" data-selectedid=""></select>
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12">
                    <label>@Resources.Language.Bank</label>
                    <input type="text" name="DateAssignment" value="" id="inputDateAssignment" class="uif-input" required="required" />
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12">
                    <label>@Resources.Language.LabelAccountType</label>
                    <input type="text" name="DateAssignment" value="" id="inputDateAssignment" class="uif-input" />
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12">
                    <label>@Resources.Language.LabelAccountNumber</label>
                    <input type="text" name="DateAssignment" value="" id="inputAccountnumber" class="uif-input" />
                </div>
            </div>
            <div class="uif-row">
                <div class="uif-col-12">
                    <label>Estado de la Cuenta</label>
                    <input type="text" name="DateAssignment" value="" id="inputDateAssignment" class="uif-input" />
                </div>
            </div>
            @*<div class="uif-modalbuttons width-save">
                <button type="button" class="btn btn-primary">@Resources.Language.Accept</button>
            </div>*@

            <div class="uif-row">
                <div class="uif-col-12 width-select">
                    <div class="pull-right">                     
                        <button type="button" class="btn btn-primary" id="btnAcceptPaymentMethods">
                            <span aria-hidden="true"></span>
                            @Resources.Language.Accept
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
