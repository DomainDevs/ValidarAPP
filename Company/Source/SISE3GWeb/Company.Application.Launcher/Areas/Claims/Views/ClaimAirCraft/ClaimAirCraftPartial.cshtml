
@{
    Layout = null;
}
<div class="uif-row">
    <div class="uif-col-12">
        <h5>
            <strong>@Resources.Language.Risks</strong>
        </h5>
    </div>
    <div class="uif-col-12">
        <table class="uif-table" data-title="@Resources.Language.RiskListing" id="tableRiskAirCraft"
               data-add="false" data-edit="false" data-delete="false" data-filter="true" data-paginate="true">
            <thead>
                <tr>
                    <th data-property="Risk"><b>@Resources.Language.Risk</b></th>
                    <th data-property="Make"><b>@Resources.Language.Make</b></th>
                    <th data-property="Model"><b>@Resources.Language.Model</b></th>
                    <th data-property="Register"><b>@Resources.Language.labelEnrollment</b></th>
                    <th data-property="Type"><b>@Resources.Language.LabelAircraftType</b></th>
                    <th data-property="Use"><b>@Resources.Language.LabelAircraftUse</b></th>
                    <th data-property="Operator"><b>@Resources.Language.LabelAircraftOperator</b></th>
                    <th data-property="InsuredAmount" data-type="currency"><b>@Resources.Language.InsuredAmount</b></th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>