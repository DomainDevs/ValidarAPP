<form id="formThirdParty">
    <div class="row">
        <div class="uif-col-12">
            <div>
                <div class="uif-row">
                    <div class="uif-col-12">


                        <div class="uif-col-6">
                            <label>@Resources.LanguagePerson.codePerson</label>
                            <div id="CodPersonIdThird" style="color: red">0</div>
                        </div>

                        <div class="uif-col-6">
                            <label>@Resources.LanguagePerson.CodeThird</label>
                            <div id="CodThird" style="color: red">0</div>
                        </div>

                    </div>
                </div>
                <div class="uif-row">
                    <div class="uif-col-12">
                        <div class="uif-col-3">
                            <label>@Resources.LanguagePerson.DateCreate</label>
                            <input type="text" class="uif-datepicker" data-type="date" data-mask="99/99/9999" id="InputDateCreate" disabled="disabled" maxlength="14" />
                        </div>
                        <div class="uif-col-3">
                            <label>@Resources.LanguagePerson.DateModifid</label>
                            <input type="text" class="uif-datepicker" data-type="date" data-mask="99/99/9999" id="InputDateModificationThird" disabled="disabled" maxlength="14" />
                        </div>
                        <div class="uif-col-3">
                            <label>@Resources.LanguagePerson.DateDeclined</label>
                            <input type="text" class="uif-datepicker" data-type="date" data-mask="99/99/9999" id="InputDateDeclinedThird" maxlength="14" />
                        </div>

                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelReasonLow</label>
                            <select class="uif-select form-control" id="selectReasonLowThird" data-id="Id" data-name="Description"></select>
                        </div>
                    </div>
                </div>


                <div class="uif-row">
                    <div class="uif-col-12">

                        <div class="uif-col-12">
                            <label>@Resources.LanguagePerson.LabelObservations</label>
                            <textarea rows="4" id="notesThird" style="resize: none" maxlength="79">
                                    </textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </form>
