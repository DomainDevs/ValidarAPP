<div data-title="@Resources.LanguagePerson.Representlegal">
    <div class="row">
        <div class="uif-col-12">
            <ul class="nav nav-tabs uif-tab-pc" id="tabsRepresenLegal">
                <li class="tabRepresentLegal" id="optionRepresentLegal">
                    <a href="#tabPersonalInfo" id="linkPersonalInfo" data-toggle="tab" class="">@Resources.LanguagePerson.PersonaInformation</a>
                </li>
                <li class="tabRepresentLegal" id="optionContactInfo">
                    <a href="#tabContactInfo" id="linkContactInfo" data-toggle="tab" class="">@Resources.LanguagePerson.LabelContactInformation</a>
                </li>
                <li class="tabRepresentLegal" id="optionRecruitmentAuthority">
                    <a href="#tabRecruitmentAuthority" id="linkRecruitmentFaculties" data-toggle="tab" class="">@Resources.LanguagePerson.LabelRecruitmentFaculties</a>
                </li>
            </ul>
            <div class="tab-content">
                <div id="tabPersonalInfo" class="tab-pane">
                    <div class="row">
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelTypeDocument</label>
                            <select id="selectDocumentTypeRepresentLegal" class="uif-select form-control" data-id="Id" data-name="SmallDescription"></select>
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelDocumentNumber</label>
                            <input type="text" id="InputNumDocumentRepresentLegal" maxlength="14" />
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelExpeditionDate</label>
                            <input type="text" class="uif-datepicker" data-type="date" data-mask="99/99/9999" data-min-date="01/01/1911"  id="InputExpeditiondateRepresentLegal" />
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelPlaceOfBirth</label>
                            <input type="text" id="InputPlaceOfBirthRepresentLegal" class="upperCase-co" maxlength="70" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="uif-col-6">
                            <label class="field-required">@Resources.LanguagePerson.LabelFullName</label>
                            <input type="text" id="InputFullNameRepresentLegal" class="upperCase-co" maxlength="120" />
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelBirthDate</label>
                            <input type="text" class="uif-datepicker" data-type="date" data-mask="99/99/9999" data-min-date="01/01/1911" id="InputBirthdateRepresentLegal" />
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelNationality</label>
                            <input type="text" id="InputNationalityRepresentLegal" class="upperCase-co" />
                        </div>
                    </div>

                </div>
                <div id="tabContactInfo" class="tab-pane">
                    <div class="row">
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelPhone</label>
                            <input type="text" id="InputPhoneReprersentLegal" />
                        </div>
                        <div class="uif-col-3">
                            <label>@Resources.LanguagePerson.LabelIsCellphone</label>
                            <input type="text" id="InputCellReprersentLegal" />
                        </div>
                        <div class="uif-col-6">
                            <label class="field-required">@Resources.LanguagePerson.Email</label>
                            <input type="text" id="InputEmailReprersentLegal" maxlength="60" class="upperCase-co" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="uif-col-6">
                            <label class="field-required">@Resources.LanguagePerson.LabelOffice</label>
                            <input type="text" id="InputOfficeReprersentLegal" class="upperCase-co" />
                        </div>
                        <div class="uif-col-6">
                            <label class="field-required">@Resources.LanguagePerson.LabelAddress</label>
                            <input type="text" id="InputAddressReprersentLegal" class="upperCase-co" maxlength="60" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelCountry</label>
                            <input class="uif-input-search" data-val="true" id="inputCountryRepresentLegal" name="inputCountryReprersentLegal" placeholder="@Resources.LanguagePerson.NameCountry" type="text">
                        </div>
                        <div class="uif-col-3">
                            <label>@Resources.LanguagePerson.LabelCodDaneCity</label>
                            <input type="text" value="" id="inputDaneCodeRepresentLegal" name="DaneCodePn" data-displaykey="DANECode" data-minlenght="2" data-maxlenght="5" placeholder="@Resources.LanguagePerson.EnterCodeDANE" />
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelState</label>
                            <input class="uif-input-search" data-val="true" id="inputStateRepresentLegal" name="inputStateReprersentLegal" placeholder="@Resources.LanguagePerson.NameDepartment" type="text">
                        </div>
                        <div class="uif-col-3">
                            <label class="field-required">@Resources.LanguagePerson.LabelCity</label>
                            <input class="uif-input-search" data-val="true" id="inputCityRepresentLegal" name="inputCityReprersentLegal" placeholder="@Resources.LanguagePerson.NameCity" type="text">
                        </div>
                    </div>
                </div>
                <div id="tabRecruitmentAuthority" class="tab-pane">
                    <div class="row">
                        <div class="uif-col-6">
                            <label>@Resources.LanguagePerson.LabelNote</label>
                            <textarea style="max-height: 100%; resize: none" id="notesReprersentLegal" class="upperCase-co" maxlength="254">
                                    </textarea>
                        </div>
                        <div class="uif-col-6">

                            <div class="row">
                                <div class="uif-col-6">
                                    <label class="field-required">@Resources.LanguagePerson.Value</label>
                                    <input type="text" class="rightAlign" id="InputvalueRepresentLegal" />
                                </div>
                                <div class="uif-col-6">
                                    <label class="field-required">@Resources.LanguagePerson.LabelCoin</label>
                                    <select class="uif-select form-control" id="selectCurrencyRepresentLegal" data-val="true" data-val-required="@Resources.LanguagePerson.ErrorDocumentControlCurrency" data-id="Id" data-name="Description" name="Currency"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
