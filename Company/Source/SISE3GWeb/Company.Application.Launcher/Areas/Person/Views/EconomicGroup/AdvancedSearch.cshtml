@model Sistran.Core.Framework.UIF.Web.Areas.Person.Models.EconomicGroupView
<script id="groupAdvancedSearchResult" type="text/template">
    <div>
        @Resources.Language.TributaryIdNo:<strong> {{TributaryIdNo}}</strong>
    </div>
    <div>
        @Resources.Language.EconomicGroupName:<strong> {{EconomicGroupName}}</strong>
    </div>
    <div>
        @Resources.Language.OperationQuoteAmount:<strong> {{OperationQuoteAmount}}</strong>
    </div>
</script>
<div class="uif-alert" id="alertAdvanced"></div>
<form class="form">
    <div class="row">
        <div class="uif-col-12">
            <h4>@Resources.Language.AdvancedSearch</h4>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-4">
            <label>@Resources.Language.EconomicGroupId</label>
            <input type="text" name="" value="" id="inputEconomicGroupId" />
        </div>
    </div>
    <div class="row">
        <div class="uif-col-6">
            <label>@Resources.Language.EconomicGroupNameLabel</label>
            <input type="text" name="" value="" id="inputEconomicGroupName" />
        </div>
        <div class="uif-col-6">
            <br />
            <input type="checkbox" name="ie" id="inputEnabled" checked="checked">
            <label for="inputEnabled">@Resources.Language.Enabled</label>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="without-label">
                <button type="button" name="name" id="btnAdvancedSearch" class="btn btn-primary-Co width-container">@Resources.Language.search</button>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="uif-listview" id="listviewSearch">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="uif-col-12">
            <div class="pull-right width-save">
                <button class="btn btn-default" id="btnCancelSearchAdv" type="button">@Resources.Language.Close</button>
                <button class="btn btn-primary" id="btnLoadProduct" type="button">@Resources.Language.Accept</button>
            </div>
        </div>
    </div>
</form>


