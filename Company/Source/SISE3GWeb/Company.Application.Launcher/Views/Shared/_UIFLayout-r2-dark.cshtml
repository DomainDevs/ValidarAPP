<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <title>UIF 2 Touch</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="google" content="notranslate" />

    @*CSSs*@
    @*<link href="@Url.Content("~/Lib/framework/css/r2/uif-touch-dark.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/Lib/framework/build/vendors/css/vendors-3.3.6.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Lib/framework/build/uif-touch-3.3.6-dark.min.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/unidaddenegocio.css")" rel="stylesheet" type="text/css" />
    @RenderSection("styles", required: false)



    <link href='http://fonts.googleapis.com/css?family=Lato:400,700,300' rel='stylesheet' type='text/css'>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->


</head>
<body>
    <div id="uif-spinner">
        <div class="spinner spinner-three-bounce">
            <i class="fa fa-circle-o-notch fa-spin fa-4x"></i>
        </div>
    </div>
    <div class="container">
        <header class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
                <img src="@Url.Content("~/Lib/framework/build/layout/dark/images/logo-sise-3g.png")" alt="">
            </div>
            <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9 nav">
                <a class="button blue" href="#" title=""><i class="fa fa-map-marker"></i></a>
                <a class="button green" href="#" title=""><i class="fa fa-calculator"></i></a>
                <a class="button red" href="#" title=""><i class="fa fa-power-off"></i></a>
                <a class="button button-big" href="#" title=""><i class="fa fa-heart"></i></a>
                <a class="button button-big" href="#" title=""><i class="fa fa-bell"></i></a>
                <a class="button button-big" href="#" title=""><i class="fa fa-user"></i></a>
            </div>
        </header><!-- /header -->
        <div class="content-wrapper">
            <nav>
                <a class="active" href="" title="">
                    <div>
                        <i class="fa fa-usd"></i> <br>
                        Caja
                    </div>
                </a>
                <a class="nav-green" href="" title="">
                    <div>
                        <i class="fa fa-usd"></i> <br>
                        Cierre
                    </div>
                </a>
                <a class="nav-red" href="" title="">
                    <div>
                        <i class="fa fa-file-text-o"></i> <br>
                        Listados
                    </div>
                </a>
                <a class="nav-light-blue" href="" title="">
                    <div>
                        <i class="fa fa-eye"></i> <br>
                        Consultas
                    </div>
                </a>
            </nav>
            <div class="content">
                @RenderBody()
            </div> <!-- content -->
        </div> <!-- content wrapper-->
    </div>

    @RenderSection("modals", required: false)
    @*JSs*@
    <script src="@Url.Content("~/Lib/framework/build/vendors/js/vendors-3.3.8.js")"></script>
    <script src="@Url.Content("~/Lib/framework/build/uif-touch-dark-3.3.8.min.js")"></script>
    <script src="@Url.Content("~/Lib/framework/build/uif-touch-4.0.0-alpha.es.js")"></script>

    <script src="@Url.Content("~/Scripts/jquery.center.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.msg.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/blockUI-2.7.0.js")" type="text/javascript"></script>

    <script type="text/javascript" src="@Url.Content("~/Accounting/Resource/Index")"></script>
    @Scripts.Render("~/bundle/js/Enums")

    @* UNEC - RECURSOS EN .JS FILE *@
    @*@Scripts.Render("~/accounting/bundles/mainBilling")*@


    <script type="text/javascript">
        var rootPath = '@Url.Content("~/")';
        var ACC_ROOT = '@Url.Content("~/Accounting/")';
        var GL_ROOT = '@Url.Content("~/GeneralLedger/")';
    </script>
    @* Fin UNEC *@

    @*JSs*@
    <script src="@Url.Content("~/Lib/framework/build/vendors/js/vendors-3.3.6.js")"></script>
    <script src="@Url.Content("~/Lib/framework/build/uif-touch-dark-3.3.6.min.js")"></script>
    <script src="@Url.Content("~/Lib/framework/build/uif-touch-3.3.6.es.js")"></script>
    <script src="@Url.Content("~/Scripts/common-validations.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/blockUI-2.7.0.js")" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
