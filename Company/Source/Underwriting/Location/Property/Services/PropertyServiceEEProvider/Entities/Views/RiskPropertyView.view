    </relation>
    <relation source="RiskBeneficiary" destination="EndorsementRisk" name="EndorsementRiskBeneficiary" type="Inner">
      <property left="RiskId" type="Equal" right="RiskId"></property>
    </relation>
    <relation source="EndorsementRisk" destination="RiskClause" name="EndorsementRiskClause" type="left">
      <property left="RiskId" type="Equal" right="RiskId"></property>
    </relation>
    <relation source="RiskInsuredObject" destination="InsuredObject" name="RiskInsuredObjectInsuredObject" type="Inner">
      <property left="InsuredObjectId" type="Equal" right="InsuredObjectId"></property>
    </relation>
  </relations>
