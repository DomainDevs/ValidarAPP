    <relation source="EndorsementRisk" destination="Policy" name="EndorsementPolicy" type="Inner">
      <property left="PolicyId" type="Equal" right="PolicyId"></property>
    </relation>
    <relation source="ConstructionCategory" destination="RiskLocation" name="ConstructionCategoryRiskLocation" type="Left">
      <property left="ConstructionCategoryCode" type="Equal" right="ConstructionCategoryCode"></property>
    </relation>
    <relation source="RiskUseEarthquakes" destination="RiskLocation" name="RiskUseEarthquakesRiskLocation" type="Left">
      <property left="RiskUseCode" type="Equal" right="RiskUseCode"></property>
    </relation>
    <relation source="Country" destination="RiskLocation" name="CountryRiskLocation" type="Left">
      <property left="CountryCode" type="Equal" right="CountryCode"></property>
    </relation>
    <relation source="State" destination="RiskLocation" name="StateRiskLocation" type="Left">
      <property left="CountryCode" type="Equal" right="CountryCode"></property>
