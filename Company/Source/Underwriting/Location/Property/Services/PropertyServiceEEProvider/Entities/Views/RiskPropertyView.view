    </relation>
    <relation source="City" destination="RiskLocation" name="CityRiskLocation" type="Left">
      <property left="CountryCode" type="Equal" right="CountryCode"></property>
      <property left="StateCode" type="Equal" right="StateCode"></property>
      <property left="CityCode" type="Equal" right="CityCode"></property>
    </relation>
    <relation source="Risk" destination="EndorsementRisk" name="EndorsementRisk" type="Inner">
      <property left="RiskId" type="Equal" right="RiskId"></property>
    </relation>
    <relation source="Risk" destination="RiskInsuredObject" name="RiskRiskInsuredObject" type="left">
