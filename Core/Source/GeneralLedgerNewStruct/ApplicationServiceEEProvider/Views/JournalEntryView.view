    <relation source="JournalEntryItem" destination="AccountingAccount" name="JournalEntryItemAccountingAccount" type="Inner">
      <property left="AccountingAccountId" type="Equal" right="AccountingAccountId"></property>
    </relation>
  </relations>
</view>