              </BinaryCriterion>
            </criteria>
          </Join>
          <ClassNameTable name="Sistran.Core.Application.Common.Entities.Bank, Core.Common.Entities" alias="RBank" />
          <criteria>
            <BinaryCriterion type="Equal">
              <Column table-alias="RBank" name="BankCode" />
              <Column table-alias="PaymentBallot" name="BankCode" />
            </BinaryCriterion>
          </criteria>
        </Join>
      </table>
