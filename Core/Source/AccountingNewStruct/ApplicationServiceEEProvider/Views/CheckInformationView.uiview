                    </BinaryCriterion>
                  </criteria>
                </Join>
                <ClassNameTable name="Sistran.Core.Application.Accounting.Entities.PaymentTicket, Core.Accounting.Entities" alias="PaymentTicket" />
                <criteria>
                  <BinaryCriterion type="Equal">
                    <Column table-alias="PaymentTicket" name="PaymentTicketCode" />
                    <Column table-alias="PaymentTicketItem" name="PaymentTicketCode" />
                  </BinaryCriterion>
                </criteria>
              </Join>
              <ClassNameTable name="Sistran.Core.Application.Accounting.Entities.PaymentTicketBallot, Core.Accounting.Entities" alias="PaymentTicketBallot" />
              <criteria>
                <BinaryCriterion type="Equal">
                  <Column table-alias="PaymentTicketBallot" name="PaymentTicketCode" />
                  <Column table-alias="PaymentTicket" name="PaymentTicketCode" />
                </BinaryCriterion>
              </criteria>
            </Join>
            <ClassNameTable name="Sistran.Core.Application.Accounting.Entities.PaymentBallot, Core.Accounting.Entities" alias="PaymentBallot" />
            <criteria>
