<?xml version="1.0" encoding="utf-8" ?>
<view xmlns="sistran:views/1.0/view.xsd" name="AgentPrefixAgentAgencyView">
  <collections>
    <collection name="AgentPrefix" type="Sistran.Core.Application.UniquePerson.Entities.AgentPrefix, 
                Core.UniquePerson.Entities"></collection>
    <collection name="Prefix" type="Sistran.Core.Application.Common.Entities.Prefix, 
                Core.Common.Entities"></collection>
	<collection name="Agent" type="Sistran.Core.Application.UniquePerson.Entities.Agent, 
                Core.UniquePerson.Entities"></collection>
    <collection name="AgentAgency" type="Sistran.Core.Application.UniquePerson.Entities.AgentAgency, 
                Core.UniquePerson.Entities"></collection>			
  </collections>
  <relations>
    <relation source="AgentPrefix" destination="Agent" name="AgentPrefixAgent" type="Inner">
      <property left="IndividualId" type="Equal" right="IndividualId"></property>
    </relation>
	<relation source="AgentPrefix" destination="Prefix" name="AgentPrefixPrefix" type="Inner">
      <property left="PrefixCode" type="Equal" right="PrefixCode"></property>
    </relation>
	<relation source="Agent" destination="AgentAgency" name="AgentAgentAgency" type="Inner">
      <property left="IndividualId" type="Equal" right="IndividualId"></property>
	  <property left="AgentTypeCode" type="Equal" right="AgentTypeCode"></property>
    </relation>
  </relations>
</view>




