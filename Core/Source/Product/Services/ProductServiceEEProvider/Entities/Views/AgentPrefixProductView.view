<?xml version="1.0" encoding="utf-8" ?>
<view xmlns="sistran:views/1.0/view.xsd" name="AgentPrefixProductView">
  <collections>
    <collection name="AgentPrefix" type="Sistran.Core.Application.UniquePerson.Entities.AgentPrefix, 
                Core.UniquePerson.Entities"></collection>
    <collection name="Product" type="Sistran.Core.Application.Product.Entities.Product, 
                Core.Product.Entities"></collection>
    <collection name="ProductAgent" type="Sistran.Core.Application.Product.Entities.ProductAgent, 
                Core.Product.Entities"></collection>
  
  </collections>
  <relations>
    <relation source="AgentPrefix" destination="Product" name="AgentPrefixProduct" type="Inner">
      <property left="PrefixCode" type="Equal" right="PrefixCode"></property>
    </relation>
    <relation source="ProductAgent" destination="Product" name="ProductAgentProduct" type="Inner">
      <property left="ProductId" type="Equal" right="ProductId"></property>     
    </relation>
  </relations>
</view>